{
  "name": "blog-starter",
  "version": "1.0.0",
  "packageManager": "yarn@4.3.1",
  "author": "Hamza Miloud Amar <hamzamiloudamar@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:HamzaAmar/amar-hamza.git",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "next:dev": "next dev --turbopack",
    "dev": "yarn next:dev",
    "build": "next build --turbopack",
    "bundle-check": "ANALYZE=true yarn build",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch",
    "lint:fix": "yarn lint --fix",
    "lint:check": "yarn lint --quiet",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "cz": "exec < /dev/tty && git cz --hook || true",
    "test": "jest",
    "validation": "yarn type-check && yarn lint:check",
    "ci": "yarn validation && yarn build",
    "clear": "concurrently \"rm -rf node_modules\" \"rm -rf .next\" \"rm -rf yarn.lock\""
  },
  "dependencies": {
    "@marsidev/react-turnstile": "^1.1.0",
    "@next/third-parties": "^15.3.2",
    "@supabase/ssr": "0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "4.1.0",
    "next": "^15.3.2",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.10.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reading-time": "^1.5.0",
    "sugar-high": "^0.9.3",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint-react/eslint-plugin": "^1.49.0",
    "@next/bundle-analyzer": "^15.3.2",
    "@next/eslint-plugin-next": "^15.3.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.18",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/rss": "0.0.32",
    "axe-playwright": "^2.1.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.3.2",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-testing-library": "^7.2.0",
    "git-cz": "^4.9.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.0.0",
    "msw": "^2.8.3",
    "rss": "^1.2.2",
    "sass": "^1.89.0",
    "schema-dts": "^1.1.5",
    "stylelint": "^16.19.1",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^7.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*": "yarn validation"
  }
}
